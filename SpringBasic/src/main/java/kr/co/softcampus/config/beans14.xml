<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	                    http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-autowire="byName">
	                    
	<bean id='obj1' class='kr.co.softcampus.beans.ch14.TestBean1'>
		<property name="data1" ref='data_bean1'/>
		<property name="data2" ref='data_bean1'/>
	</bean>
	
	<bean id='data_bean1' class='kr.co.softcampus.beans.ch14.DataBean1' scope='prototype'/>
	
	<bean id='obj2' class='kr.co.softcampus.beans.ch14.TestBean1' autowire="byName"/>
	
	<bean id='data1' class='kr.co.softcampus.beans.ch14.DataBean1'/>
	<bean id='data2' class='kr.co.softcampus.beans.ch14.DataBean1'/>
	
	<bean id='obj3' class='kr.co.softcampus.beans.ch14.TestBean2' autowire="byType"/>
	
	<bean id='data_bean2' class='kr.co.softcampus.beans.ch14.DataBean2'/>
	<!-- <bean class='kr.co.softcampus.beans.ch14.DataBean2'/>-->
	
	<bean id='obj4' class='kr.co.softcampus.beans.ch14.TestBean3'>
		<constructor-arg ref="data_bean2" index="0"/>
		<constructor-arg ref='data_bean2' index='1'/>
	</bean>
	
	<bean id='obj5' class='kr.co.softcampus.beans.ch14.TestBean3' autowire="constructor"/>
	
	<bean id='obj6' class='kr.co.softcampus.beans.ch14.TestBean3'>
		<constructor-arg value='100' index='0' type='int'/>
		<constructor-arg value='문자열' index='1'/>
		<constructor-arg ref='data_bean2' index='2'/>
		<constructor-arg ref='data_bean2' index='3'/>
	</bean>
	
	<bean id='obj7' class='kr.co.softcampus.beans.ch14.TestBean3' autowire="constructor">
		<constructor-arg value='200' index='0' type='int'/>
		<constructor-arg value='문자열2' index='1'/>
	</bean>
	
	<bean id='obj8' class='kr.co.softcampus.beans.ch14.TestBean1'/>
	
	<bean id='obj9' class='kr.co.softcampus.beans.ch14.TestBean1' autowire="no"/>
</beans>












